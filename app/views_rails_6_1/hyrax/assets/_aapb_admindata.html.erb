<% if presenter.display_aapb_admin_data? %>

  <div class="block">
    <a class="btn btn-default btn-block header collapsed" role="button" data-toggle="collapse" href="#collapse-<%= t('.aapb_admin').parameterize.underscore %>" aria-expanded="false" aria-controls="collapse-<%= t('.aapb_admin').parameterize.underscore %>">
        <%= t('.aapb_admin') %>
    </a>
    <div class="collapse" id="collapse-<%= t('.aapb_admin').parameterize.underscore %>">
      <div class="well">
        <dl>
          <%= presenter.attribute_to_html(:sonyci_id, html_dl: true) %>
          <%= presenter.attribute_to_html(:last_pushed, html_dl: true) %>
          <%= render 'batch', presenter: presenter %>
        </dl>
      </div>
    </div>
  </div>
<% end %>
