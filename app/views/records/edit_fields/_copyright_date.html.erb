<% if f.object.multiple? key %>
  <%= f.input :copyright_date, as: :multiple_date,wrapper_html: { class: 'multi_value' }, input_html: { class: 'form-control' }, required: f.object.required?(key) %>
<% else %>
  <%= f.input :copyright_date, as: :date, html5: true , required: f.object.required?(key) %>
<% end %>