<% if f.object.multiple? key %>
  <%= f.input :date, as: :multiple_date,wrapper_html: { class: 'multi_value' }, input_html: { class: 'form-control' }, required: f.object.required?(key), disabled:f.object.respond_to?(:readonly?) && f.object.readonly?(key) %>
<% else %>
  <%= f.input :date, as: :date, html5: true, disabled:f.object.respond_to?(:readonly?) && f.object.readonly?(key) , required: f.object.required?(key) %>
<% end %>