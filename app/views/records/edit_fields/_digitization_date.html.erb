<% if f.object.multiple? key %>
  <%= f.input :digitization_date, as: :multiple_date,wrapper_html: { class: 'multi_value' }, input_html: { class: 'form-control' }, disabled:f.object.respond_to?(:readonly?) && f.object.readonly?(key), required: f.object.required?(key) %>
<% else %>
  <%= f.input :digitization_date, as: :date, disabled:f.object.respond_to?(:readonly?) && f.object.readonly?(key), html5: true, required: f.object.required?(key) %>
<% end %>